{
    "app": {
      "environment": "test"
    },
    "logger": {
      "level": "info",
      "format": "json",
      "includeSensitive": false
    },
    "dynamoDB": {
      "tokenTable": "TidalTokens-test",
      "playbackTable": "TidalPlaybackState-test"
    },
